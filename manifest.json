{
  "manifest_version": 2,
  "name": "RollRes - Change the response of the request",
  "version": "0.0.2",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "description": "Change the response of the request.",
  "icons": {
    "128": "128.png",
    "32": "32.png"
  },
  "browser_action": {
    "default_icon": "128.png",
    "default_title": "RollRes - Change the response of the request",
    "default_popup": "popup.html"
  },
  "options_page": "option.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
